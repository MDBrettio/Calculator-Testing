public class KineticEnergy{

     public static void main(String []args){
         
double a=.5;
//.5 to be multiplied with mass. This never changes
double b=215;
//mass
double c=51;
//Velocity
double d=c*c;
//velocity squared
double e=(a*b)*d;

System.out.println("Kinetic Energy = "+e+" Joules");

         
     }
}
